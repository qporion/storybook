{
    "require": {
        "sensiolabs/storybook-bundle": "^0.1.1",
        "symfony/runtime": "^7.1",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.1",
        "symfony/yaml": "^7.1",
        "symfony/ux-twig-component": "^2.18",
        "symfony/stimulus-bundle": "^2.18"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
